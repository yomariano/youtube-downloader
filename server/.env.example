# Server Configuration
PORT=3001
NODE_ENV=production

# BrightData Proxy (formerly Luminati)
BRIGHTDATA_HOST=brd.superproxy.io
BRIGHTDATA_PORT=22225
BRIGHTDATA_USERNAME=brd-customer-hl_XXXXXXX-zone-residential
BRIGHTDATA_PASSWORD=your_password_here

# Alternative: Smartproxy
SMARTPROXY_USERNAME=
SMARTPROXY_PASSWORD=

# Alternative: Oxylabs
OXYLABS_USERNAME=
OXYLABS_PASSWORD=

# Scaling Configuration
MAX_RETRY_ATTEMPTS=3
PROXY_TIMEOUT=30000
ENABLE_PROXY_ROTATION=true

# Rate Limiting (requests per minute)
RATE_LIMIT=30